<%= content_for :title do %>
  <%= t "page.front-end.home-page.title" %>
<% end %>
<div class="row">
  <table class="striped centered highlight" id="table-user">
    <thead>
      <tr>
        <th data-field="stt">
          <%= t "page.admin.users.stt" %>
        </th>
        <th data-field="name">
          <%= t "page.admin.users.name" %>
        </th>
        <th data-field="description">
          <%= t "page.admin.users.email" %>
        </th>
      </tr>
    </thead>
    <tbody class="user_body_tables">
      <% byebug %>
      <% @users.each_with_index do |user, index| %>
        <tr>
          <td><%= index+1 %></td>
          <td><%= user.name %></td>
          <td><%= user.email %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
  <%= paginate @users %>
</div>

